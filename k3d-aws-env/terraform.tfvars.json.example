    "k3d_servers": 1,
    "k3d_agents": 3,
    "k3d_api_port": 6550,
    "k3d_upload_images": false,
    "k3d_images_tarball": "<project_full_path>/hack/images.tar.gz",
    "ansible_post_args": "-v"
}