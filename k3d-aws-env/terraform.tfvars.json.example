{
    "private_key_path": "<home_full_path>/.ssh/id_rsa",
    "public_key_path": "<home_full_path>/.ssh/id_rsa.pub",
    "download_kubeconfig": true,
    "kubeconfig_path": "<home_full_path>/.kube/config",
    "k3d_cluster_name": "dev-env",
